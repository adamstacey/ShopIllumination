{% for child in form %}
    {% if child.vars.name == 'displayOrder' %}
        <td width="1"><img class="handle" src="{{ asset('bundles/kacsite/images/icons/handle.png') }}" border="0" /></td>
    {% endif %}
{% endfor %}
<td class="row-count" width="1"></td>
{% for child in form %}
    {% if child.vars.block_prefixes.1 != 'hidden' %}
        <td{% if child.vars.attr['data-container-class'] is defined %} class="{{ child.vars.attr['data-container-class'] }}"{% endif %}>{{ form_widget(child) }}</td>
    {% endif %}
    {% if child.vars.attr['data-apply-to-all'] is defined %}
        <td width="1" class="tar">
            <button type="button" data-apply-to-all-field="{{ child.vars.attr['data-apply-to-all'] }}" title="Click to apply this value to all others." class="actionApplyToAll button icon-grey" data-icon-primary="icon-752" data-icon-only="true">Apply to All</button>
        </td>
    {% endif %}
{% endfor %}
<td class="tac" width="1">
    {{ form_rest(form) }}
    <span title="Delete" class="actionDeleteFormRow link ui-icon ui-icon-small icon-small-1118 icon-red"></span>
</td>