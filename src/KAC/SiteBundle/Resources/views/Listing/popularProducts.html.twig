<div class="popular_products popular_items">
    {% for item in products %}
        {% set product = item[0] %}
        <div class="popular_product popular_item">
            <a itemprop="name" href="{{ product.url != '' ? path('routing', {'url': product.url}) : '' }}">{{ product.description.header|raw }}</a>
        </div>
    {% endfor %}
</div>