<table cellspacing="0" border="0" style="background-color: #FFFFFF;" cellpadding="0" width="100%">
<tr>
<td valign="top">
<table width="100%" cellspacing="0" border="0" align="center" style="background: #FFFFFF; border: 1px solid #CCCCCC;"
       cellpadding="0">
<tr>
<td valign="top">
<table width="100%" cellspacing="0" border="0" cellpadding="0">
<tr>
<td width="100%" valign="top" style="padding: 10px;">
<table width="100%" style="border-collapse: collapse;" cellspacing="0" border="0" cellpadding="0">
    <tr>
        <td width="300" valign="top" align="left" style="background: #FFFFFF;"><img height="50"
                                                                                    alt="Kitchen Appliance Centre"
                                                                                    border="0"
                                                                                    src="http://www.kitchenappliancecentre.co.uk/bundles/webilluminationshop/images/logos/kitchen-appliance-centre-email-logo.png"/>
        </td>
        <td valign="top" align="right"
            style="line-height: 120%; background: #FFFFFF; font-weight: bold; color: #000000; font-size: 24px; font-family: Arial;">
            ORDER: <span
                    style="line-height: 120%; font-weight: normal; color: #444444; font-size: 24px; font-family: Arial;">{{ order.orderNumber }}</span><br/><span
                    style="line-height: 120%; font-weight: normal; color: #676767; font-size: 16px; font-family: Arial;">{{ order.orderDate|date('d/m/Y h:iA') }}</span>
        </
        </td>
    </tr>
</table>
<table width="100%" style="border-collapse: collapse;" cellspacing="0" border="0" cellpadding="0">
    <tr>
        <td width="50%" valign="top" align="left" style="background: #FFFFFF; padding: 0 5px 0 0;">
            <table width="100%" style="border-collapse: collapse;" cellspacing="0" border="0" cellpadding="0">
                <tr>
                    <td valign="top" align="left" style="background: #FFFFFF;">
                        <h3 style="border-bottom: 1px dotted #676767; line-height: 100%; color: #000; font-size: 14px; font-family: Arial; padding: 20px 0px 5px 0px; margin: 0px 0px 5px 0px;">
                            Billing Information</h3>
                    </td>
                </tr>
                <tr>
                    <td valign="top" align="left" style="background: #FFFFFF;">
                        <table width="100%" style="border-collapse: collapse; border: 1px solid #CCC;" cellspacing="0"
                               border="0" cellpadding="0">
                            <tr>
                                <td width="110" valign="top" align="left"
                                    style="white-space: nowrap; border: 1px solid #CCC; line-height: 120%; background: #DDDDDD; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                                    <strong>Billing Address</strong></td>
                                <td valign="top" align="left"
                                    style="text-transform: uppercase; border: 1px solid #CCC; line-height: 120%; background: #FFF; color: #999; font-size: 10px; font-family: Arial; padding: 2px 5px;">{% spaceless %}
                                        {{ order.billingFirstName }} {{ order.billingLastName }}<br/>
                                        {% if order.billingOrganisationName != '' %}
                                            {{ order.billingOrganisationName }}<br/>
                                        {% endif %}
                                        {{ order.billingAddressLine1 }}<br/>
                                        {% if order.billingAddressLine2 != '' %}
                                            {{ order.billingAddressLine2 }}<br/>
                                        {% endif %}
                                        {{ order.billingTownCity }}<br/>
                                        {% if order.billingCountyState != '' %}
                                            {{ order.billingCountyState }}<br/>
                                        {% endif %}
                                        {% if order.billingCountryCode == 'GB' %}
                                            {{ order.billingPostZipCode }}<br/>
                                        {% endif %}
                                        {% if order.billingCountryCode == 'IE' %}
                                            Republic of Ireland
                                        {% endif %}
                                    {% endspaceless %}</td>
                            </tr>
                            <tr>
                                <td width="110" valign="top" align="left"
                                    style="white-space: nowrap; border: 1px solid #CCC; line-height: 120%; background: #DDDDDD; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                                    <strong>Telephone (Daytime)</strong></td>
                                <td valign="top" align="left"
                                    style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px;">{{ order.telephoneDaytime }}</td>
                            </tr>
                            {% if order.telephoneEvening %}
                                <tr>
                                    <td width="110" valign="top" align="left"
                                        style="white-space: nowrap; border: 1px solid #CCC; line-height: 120%; background: #DDDDDD; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                                        <strong>Telephone (Evening)</strong></td>
                                    <td valign="top" align="left"
                                        style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px;">{{ order.telephoneEvening }}</td>
                                </tr>
                            {% endif %}
                            {% if order.mobile %}
                                <tr>
                                    <td width="110" valign="top" align="left"
                                        style="white-space: nowrap; border: 1px solid #CCC; line-height: 120%; background: #DDDDDD; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                                        <strong>Mobile</strong></td>
                                    <td valign="top" align="left"
                                        style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px;">{{ order.mobile }}</td>
                                </tr>
                            {% endif %}
                            <tr>
                                <td width="110" valign="top" align="left"
                                    style="white-space: nowrap; border: 1px solid #CCC; line-height: 120%; background: #DDDDDD; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                                    <strong>Email Address</strong></td>
                                <td valign="top" align="left"
                                    style="border: 1px solid #CCC; text-transform: lowercase; line-height: 120%; background: #FFFFFF; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px;">{{ order.emailAddress }}</td>
                            </tr>
                            <tr>
                                <td width="110" valign="top" align="left"
                                    style="white-space: nowrap; border: 1px solid #CCC; line-height: 120%; background: #DDDDDD; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                                    <strong>Payment Type</strong></td>
                                <td valign="top" align="left"
                                    style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px;">{{ order.paymentType }}</td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
            {% if (order.paymentType == 'PayPal') or (order.paymentType == 'PayPal through SagePay') %}
                <table width="100%" style="border-collapse: collapse; border: 2px solid #CC0000; margin: 5px 0 0 0;"
                       cellspacing="0" border="0" cellpadding="0">
                    <tr>
                        <td width="82" valign="top" align="left"
                            style="white-space: nowrap; border: 2px solid #CC0000; line-height: 120%; background: #CC0000; color: #FFF; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                            <strong>PayPal Checked</strong></td>
                        <td valign="top" align="center"
                            style="text-transform: uppercase; border: 2px solid #CC0000; line-height: 120%; background: #FFFFFF; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px;">
                            <div style="border: 1px solid #000; width: 10px; height: 10px;"></div>
                        </td>
                        <td width="60" valign="top" align="left"
                            style="white-space: nowrap; border: 2px solid #CC0000; line-height: 120%; background: #CC0000; color: #FFF; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                            <strong>Checked By</strong></td>
                        <td valign="top" align="left"
                            style="text-transform: uppercase; border: 2px solid #CC0000; line-height: 120%; background: #FFFFFF; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px;">
                            &nbsp;</td>
                    </tr>
                </table>
            {% endif %}
        </td>
        <td width="50%" valign="top" style="background: #FFFFFF; padding: 0 0 0 5px;">
            <table width="100%" style="border-collapse: collapse;" cellspacing="0" border="0" cellpadding="0">
                <tr>
                    <td valign="top" align="left" style="background: #FFFFFF;">
                        <h3 style="border-bottom: 2px dotted #CC0000; line-height: 100%; color: #CC0000; font-size: 14px; font-family: Arial; padding: 20px 0px 5px 0px; margin: 0px 0px 5px 0px;">
                            DELIVERY INFORMATION</h3>
                    </td>
                </tr>
                <tr>
                    <td valign="top" align="left" style="background: #FFFFFF;">
                        <table width="100%" style="border-collapse: collapse; border: 1px solid #CCC;" cellspacing="0"
                               border="0" cellpadding="0">
                            <tr>
                                <td width="110" valign="top" align="left"
                                    style="white-space: nowrap; border: 1px solid #CCC; line-height: 120%; background: #DDDDDD; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                                    <strong>Delivery{% if order.deliveryType != 'Collection' %} Address{% endif %}</strong>
                                </td>
                                {% if order.deliveryType == 'Collection' %}
                                    <td valign="top" align="left"
                                        style="text-transform: uppercase; border: 1px solid #CCC; line-height: 120%; background: #CC0000; color: #FFF; font-size: 12px; font-weight: bold; font-family: 'Arial Black', Arial; padding: 2px 5px;">
                                        COLLECTION
                                    </td>
                                {% else %}
                                    <td valign="top" align="left"
                                        style="text-transform: uppercase; border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #CC0000; font-size: 12px; font-weight: bold; font-family: Arial; padding: 2px 5px;">{% spaceless %}
                                            {{ order.deliveryFirstName }} {{ order.deliveryLastName }}<br/>
                                            {% if order.deliveryOrganisationName != '' %}
                                                {{ order.deliveryOrganisationName }}<br/>
                                            {% endif %}
                                            {{ order.deliveryAddressLine1 }}<br/>
                                            {% if order.deliveryAddressLine2 != '' %}
                                                {{ order.deliveryAddressLine2 }}<br/>
                                            {% endif %}
                                            {{ order.deliveryTownCity }}<br/>
                                            {% if order.deliveryCountyState != '' %}
                                                {{ order.deliveryCountyState }}<br/>
                                            {% endif %}
                                            {% if order.deliveryCountryCode == 'GB' %}
                                                {{ order.deliveryPostZipCode }}<br/>
                                            {% endif %}
                                            {% if order.deliveryCountryCode == 'IE' %}
                                                Republic of Ireland
                                            {% endif %}
                                        {% endspaceless %}</td>
                                {% endif %}
                            </tr>
                            {% if order.deliveryType != 'Collection' %}
                                <tr>
                                    <td width="110" valign="top" align="left"
                                        style="white-space: nowrap; border: 1px solid #CCC; line-height: 120%; background: #DDDDDD; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                                        <strong>Delivery Type</strong></td>
                                    <td valign="top" align="left"
                                        style="border: 1px solid #CCC; line-height: 120%; {% if order.deliveryType == 'Collection' %}background: #CC0000; color: #FFF; font-weight: bold; {% else %}background: #FFFFFF; color: #CC0000; {% endif %}font-size: 10px; font-family: Arial; padding: 2px 5px;">{{ order.deliveryType }}</td>
                                </tr>
                                <tr>
                                    <td width="110" valign="top" align="left"
                                        style="white-space: nowrap; border: 1px solid #CCC; line-height: 120%; background: #DDDDDD; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                                        <strong>Estimated Delivery</strong></td>
                                    <td valign="top" align="left"
                                        style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #CC0000; font-size: 10px; font-family: Arial; padding: 2px 5px; text-transform: uppercase;">
                                        <strong>{{ order.estimatedDeliveryDaysStart }}
                                            <br/>{{ order.estimatedDeliveryDaysEnd }}</strong></td>
                                </tr>
                            {% endif %}
                        </table>
                    </td>
                </tr>
            </table>
            {% if order.deliveryType != 'Collection' %}
                <table width="100%" style="border-collapse: collapse; border: 2px solid #CC0000; margin: 5px 0 0 0;"
                       cellspacing="0" border="0" cellpadding="0">
                    <tr>
                        <td width="106" valign="top" align="left"
                            style="white-space: nowrap; border: 2px solid #CC0000; line-height: 120%; background: #DDDDDD; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                            <strong>Timeframes Checked</strong></td>
                        <td valign="top" align="center"
                            style="text-transform: uppercase; border: 2px solid #CC0000; line-height: 120%; background: #FFFFFF; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px;">
                            <div style="border: 1px solid #000; width: 10px; height: 10px;"></div>
                        </td>
                        <td width="60" valign="top" align="left"
                            style="white-space: nowrap; border: 2px solid #CC0000; line-height: 120%; background: #DDDDDD; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                            <strong>Checked By</strong></td>
                        <td valign="top" align="left"
                            style="text-transform: uppercase; border: 2px solid #CC0000; line-height: 120%; background: #FFFFFF; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px;">
                            &nbsp;</td>
                    </tr>
                </table>
            {% endif %}
        </td>
    </tr>
</table>
{% if order.customerNotes|length > 0 %}
    <h3 style="border-bottom: 1px dotted #676767; line-height: 100%; color: #000; font-size: 14px; font-family: Arial; padding: 20px 0px 5px 0px; margin: 0px 0px 0px 0px;">
        Notes</h3>
    {% for note in order.customerNotes %}
        <p style="font-family: Arial; font-size: 11px; color: #CC0000; padding: 5px 0 0 0; margin: 0; line-height: 100%;">
            <em>{{ note.note }}</em></p>
    {% endfor %}
{% endif %}
<h3 style="border-bottom: 1px dotted #676767; line-height: 100%; color: #000; font-size: 14px; font-family: Arial; padding: 20px 0px 5px 0px; margin: 0px 0px 5px 0px;">
    Items Ordered</h3>
<table style="border-collapse: collapse; border: 1px solid #CCC;" cellspacing="0" border="0" cellpadding="0"
       width="100%">
    <tr>
        <td width="1" colspan="2" valign="top" align="left"
            style="border: 1px solid #CCC; text-align: center; line-height: 120%; background: #DDDDDD; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; font-weight: bold;"
            align="center" width="180"><strong>Quantity</strong></td>
        <td valign="top" align="left"
            style="border: 1px solid #CCC; text-align: center; line-height: 120%; background: #DDDDDD; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; font-weight: bold;"
            align="center" width="180"><strong>Product</strong></td>
        <td width="1" valign="top" align="left"
            style="border: 1px solid #CCC; text-align: center; line-height: 120%; background: #DDDDDD; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; font-weight: bold;"
            align="center" width="180"><strong>Code</strong></td>
        <td width="1" colspan="3" valign="top" align="left"
            style="border: 1px solid #CCC; text-align: center; line-height: 120%; background: #DDDDDD; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; font-weight: bold;"
            align="center" width="180"><strong>Price</strong></td>
    </tr>
    {% for product in order.products %}
        <tr>
            <td valign="top"
                style="border: 1px solid #CCC; line-height: 120%;{% if product.quantity > 1 %} background: #CC0000; color: #FFF; font-weight: bold;{% else %} background: #FFFFFF; color: #000;{% endif %} font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: center;"
                align="center" width="1">{{ product.quantity }}</td>
            <td valign="top"
                style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: center;"
                align="center" width="1">&times;</td>
            <td valign="top" align="left"
                style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-transform: uppercase;">{% spaceless %}
                    <strong>{{ product.header }}</strong>
                    {% if product.selectedOptionLabels|length > 0 %}
                        <br/>
                        {% for selectedOptionLabel in product.selectedOptionLabels %}
                            {% if selectedOptionLabel != '' %}
                                &nbsp;&nbsp;-&nbsp;&nbsp;{{ selectedOptionLabel|raw }}{% if not loop.last %}
                                <br/>{% endif %}
                            {% endif %}
                        {% endfor %}
                    {% endif %}
                {% endspaceless %}</td>
            <td valign="top"
                style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: center;"
                align="center" width="1">{{ product.productCode }}</td>
            <td valign="top"
                style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: center;"
                align="center" width="1">&pound;{{ product.unitCost|number_format(2) }}</td>
            <td valign="top"
                style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: center;"
                align="center" width="1">=
            </td>
            <td valign="top"
                style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: center;"
                align="center" width="1">&pound;{{ product.subTotal|number_format(2) }}</td>
        </tr>
    {% endfor %}
</table>
<table style="border-collapse: collapse; border: 1px solid #CCC;" width="100%" cellspacing="0" border="0"
       cellpadding="0">
    <tr>
        <td valign="top"
            style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: right;"
            align="right"><strong>Subtotal before Delivery Charges (inc. VAT):</strong></td>
        <td valign="top"
            style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #CC0000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: right;"
            align="right" width="1"><strong>&pound;{{ order.subTotal|number_format(2) }}</strong></td>
    </tr>
    <tr>
        <td valign="top"
            style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: right;"
            align="right"><strong>Delivery Charge (inc. VAT):</strong></td>
        {% if order.deliveryType == 'Collection' %}
            <td valign="top"
                style="border: 1px solid #CCC; line-height: 120%; background: #CC0000; color: #FFFFFF; font-weight: bold; font-size: 11px; font-family: 'Arial Black', Arial; padding: 2px 5px; text-align: right;"
                align="right" width="1">COLLECTION
            </td>
        {% else %}
            {% if order.deliveryCharge == 0 %}
                <td valign="top"
                    style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #009900; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: right;"
                    align="right" width="1">FREE
                </td>
            {% else %}
                <td valign="top"
                    style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #CC0000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: right;"
                    align="right" width="1">&pound;{{ order.deliveryCharge|number_format(2) }}</td>
            {% endif %}
        {% endif %}
    </tr>
    {% if order.discount > 0 %}
        <tr>
            <td valign="top"
                style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: right;"
                align="right"><strong>Discount:</strong></td>
            <td valign="top"
                style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #009900; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: right;"
                align="right" width="1">&pound;{{ order.discount|number_format(2) }}</td>
        </tr>
    {% endif %}
    <tr>
        <td valign="top"
            style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: right;"
            align="right"><strong>VAT (20%):</strong></td>
        <td valign="top"
            style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #CC0000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: right;"
            align="right" width="1"><strong>&pound;{{ order.vat|number_format(2) }}</strong></td>
    </tr>
    <tr>
        <td valign="top"
            style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: right;"
            align="right"><strong>Total Paid (inc. VAT):</strong></td>
        <td valign="top"
            style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #CC0000; font-size: 11px; font-family: Arial; padding: 2px 5px; text-align: right;"
            align="right" width="1"><strong>&pound;{{ order.total|number_format(2) }}</strong></td>
    </tr>
</table>
{% if (order.deliveryType == 'Royal Mail 1st Class') or (order.deliveryType == 'Royal Mail Economy') or (order.deliveryType == 'Parcel Delivery Express') or (order.deliveryType == 'Parcel Delivery Economy') or (order.deliveryType == 'FREE DELIVERY Parcel Service') or (order.deliveryType == 'FREE DELIVERY Parcel Express') %}
    <h3 style="border-bottom: 1px dotted #676767; line-height: 100%; color: #000; font-size: 14px; font-family: Arial; padding: 20px 0px 5px 0px; margin: 0px 0px 5px 0px;">{% if (order.deliveryType == 'Royal Mail 1st Class') or (order.deliveryType == 'Royal Mail Economy') %}Royal Mail{% else %}Parcel Delivery{% endif %}
        Checklist</h3>
    <table style="border-collapse: collapse; border: 2px solid #CC0000;" width="100%" cellspacing="0" border="0"
           cellpadding="0">
        <tr>
            <td width="50%" valign="top" align="left"
                style="border: 2px solid #CC0000; text-align: left; line-height: 120%; background: #CCC; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                <strong>SALES</strong></td>
            <td width="25%" valign="top" align="left"
                style="border: 2px solid #CC0000; text-align: left; line-height: 120%; background: #CCC; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                <strong>WAREHOUSE</strong></td>
            <td width="25%" valign="top" align="left"
                style="border: 2px solid #CC0000; text-align: left; line-height: 120%; background: #CCC; color: #000; font-size: 11px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                <strong>DELIVERY</strong></td>
        </tr>
        <tr>
            <td width="50%" valign="top" align="left" style="border: 2px solid #CC0000; padding: 5px;">
                <table width="100%" style="border-collapse: collapse; border: 1px solid #CCC;" cellspacing="0"
                       border="0" cellpadding="0">
                    <tr>
                        <td width="80" valign="top" align="left"
                            style="white-space: nowrap; border: 1px solid #CCC; line-height: 120%; background: #DDDDDD; color: #000; font-size: 10px; font-family: Arial; padding: 2px 5px; font-weight: bold;">
                            <strong>CSY Processed</strong></td>
