{% extends 'KACSiteBundle::base.html.twig' %}
{% block body %}
    <style>
        .add-to-basket {
            width: 110px;
            height: 40px;

            display: block;
            position: relative;
        }

        .add-to-basket input.quantity {
            width: 40px;

            position: absolute;
            top: 0;
            left: 0;

            text-align: center;
            border-radius: 4px 0 0 4px;
        }

        .add-to-basket .buy {
            position: absolute;
            top: 0;
            right: 0;
        }

        .add-to-basket .increase-quantity,
        .add-to-basket .decrease-quantity {
            position: absolute;
            left: 40px;
        }

        .add-to-basket .increase-quantity .ui-button-text,
        .add-to-basket .decrease-quantity .ui-button-text {
            width: 10px;
            height: 12px;
            padding: 0 2px;

            display: block;

            line-height: 12px;
            text-align: center;
        }

        .add-to-basket .increase-quantity {
            top: 0;
            border-radius: 0 4px 0 0;
        }

        .add-to-basket .decrease-quantity {
            bottom: 0;
            border-radius: 0 0 4px 0;
        }

        .add-to-basket .buy .ui-button-text {
            padding: 0 2px;
        }

        .product .form-helper {
            position: relative;
            top: auto;
            left: auto;
            right: auto;
        }

        .product td p {
            line-height: 150%;
            margin: 0;
        }

        .ui-widget-content p {
            line-height: 150%;
        }
    </style>
    <script>
        function selectWorktopThickness(worktopThickness) {
            $("table.data-table tbody tr").hide();
            $("table.data-table tbody tr").removeClass("even odd");
            $("table.data-table tbody tr[data-worktop-thickness='"+worktopThickness+"']").show();
            $("table.data-table tbody tr:visible:even").addClass("even");
            $("table.data-table tbody tr:visible:odd").addClass("odd");
        }

        $(document).ready(function() {
            selectWorktopThickness($("input[name='worktop-thickness']").val());

            $(".actionIncreaseQuantity").on("click", function() {
                var $quantityInput = $(this).parent().find("input.quantity");
                $quantityInput.val(parseInt($quantityInput.val()) + 1);
            });

            $(".actionDecreaseQuantity").on("click", function() {
                var $quantityInput = $(this).parent().find("input.quantity");
                if (parseInt($quantityInput.val()) > 1) {
                    $quantityInput.val(parseInt($quantityInput.val()) - 1);
                }
            });

            $(".actionAddToBasket").on("click", function() {
                $(this).closest("tr").find("td:not(.image)").css("background", "#ebf7eb");
            });

            $("input[name='worktop-thickness']").on("change", function() {
                selectWorktopThickness($(this).val());
            });
        });
    </script>
    {% import _self as helpers %}
    {% macro addToBasket() %}
        <div class="add-to-basket">
            <input class="quantity integer" type="number" value="1" />
            <button class="actionIncreaseQuantity increase-quantity button button-small uppercase">+</button>
            <button class="actionDecreaseQuantity decrease-quantity button button-small uppercase">-</button>
            <button class="actionAddToBasket buy button button-green uppercase">Buy</button>
        </div>
