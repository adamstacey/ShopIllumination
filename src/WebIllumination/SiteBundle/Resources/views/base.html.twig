<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" >
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta name="google-site-verification" content="iEztzaBnI5VsikHe3OXqS_r3RpPyiwtCKLQnacqFu8A" />
    <title>{% block title %}Kitchen Appliance Centre{% endblock %}</title>
    {% block metatags %}
        <meta name="description" content="Kitchen Appliance Centre - A wide selection of kitchen appliances including Kitchen Sinks, Range Cookers, Fridges, Freezers, Dishwashers, Washing Machines, Worktops, Ovens and Hobs, all at low affordable prices.">
        <meta name="keywords" content="worktops, charcoal filters, cooker hoods, built-in microwaves, integrated dishwashers, integrated washing machines, refrigeration, cleaning products, glass splashbacks, waste disposer, sinks, taps, plinth heaters, range cookers, ducting, hobs, ovens, coffee machines, solid surface worktops, kitchen appliances">
    {% endblock %}
    {% block stylesheets %}
        {% stylesheets
            '@WebIlluminationShopBundle/Resources/public/css/reset.css'
            '@WebIlluminationShopBundle/Resources/public/css/grid.css'
            '@WebIlluminationShopBundle/Resources/public/css/ui.css'
            '@WebIlluminationShopBundle/Resources/public/css/portlet.css'
            '@WebIlluminationShopBundle/Resources/public/css/uniform.css'
            '@WebIlluminationShopBundle/Resources/public/css/slider.css'
            '@WebIlluminationShopBundle/Resources/public/css/form.css'
            '@WebIlluminationShopBundle/Resources/public/css/styles.css'
            output='css/shop-compressed.css'
            filter='yui_css'
        %}
            <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
        {% endstylesheets %}
        <!--[if IE]>
            {% stylesheets '@WebIlluminationSiteBundle/Resources/public/css/ie.css' output='css/ie.css' filter='yui_css' %}
                <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
            {% endstylesheets %}
        <![endif]-->
        <!--[if gte IE 9]>
            {% stylesheets '@WebIlluminationSiteBundle/Resources/public/css/ie9.css' output='css/ie9.css' filter='yui_css' %}
                <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
            {% endstylesheets %}
        <![endif]-->
        <!--[if IE 8]>
            {% stylesheets '@WebIlluminationSiteBundle/Resources/public/css/ie8.css' output='css/ie8.css' filter='yui_css' %}
                <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
            {% endstylesheets %}
        <![endif]-->
        <!--[if IE 7]>
            {% stylesheets '@WebIlluminationSiteBundle/Resources/public/css/ie7.css' output='css/ie7.css' filter='yui_css' %}
                <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
            {% endstylesheets %}
        <![endif]-->
    {% endblock %}
    {% block headerscripts %}
        <!--[if lt IE 9]>
        {% javascripts '@WebIlluminationShopBundle/Resources/public/js/html5.js' output='js/shop-html5.js' filter='yui_js' %}
        <script src="{{ asset_url }}" type="text/javascript"></script>
        {% endjavascripts %}
        <![endif]-->
        {% javascripts
        '@WebIlluminationShopBundle/Resources/public/js/jquery.js'
        '@WebIlluminationShopBundle/Resources/public/js/ui.js'
        '@WebIlluminationShopBundle/Resources/public/js/tools.js'
        '@WebIlluminationShopBundle/Resources/public/js/overlay.js'
        '@WebIlluminationShopBundle/Resources/public/js/uniform.js'
        '@WebIlluminationShopBundle/Resources/public/js/slider.js'
        '@WebIlluminationShopBundle/Resources/public/js/cookies.js'
        '@WebIlluminationShopBundle/Resources/public/js/global.js'
        output='js/shop-compressed.js' filter='yui_js' %}
        <script src="{{ asset_url }}" type="text/javascript"></script>
        {% endjavascripts %}
        <!--[if lt IE 9]>
        {% javascripts '@WebIlluminationShopBundle/Resources/public/js/selectivizr.js' output='js/shop-selectivizr.js' filter='yui_js' %}
        <script src="{{ asset_url }}" type="text/javascript"></script>
        {% endjavascripts %}
        <![endif]-->
        <!--[if lt IE 8]>
        {% javascripts '@WebIlluminationShopBundle/Resources/public/js/ie.js' output='js/shop-ie.js' filter='yui_js' %}
        <script src="{{ asset_url }}" type="text/javascript"></script>
        {% endjavascripts %}
        <![endif]-->
    {% endblock %}
    <link rel="shortcut icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <link rel="apple-touch-icon" href="{{ asset('apple-touch-icon.png') }}">
    {% include '::shopTracking.html.twig' %}
</head>
<body itemscope itemtype="http://schema.org/WebPage">
<div id="wrapper" class="clearfix">
    {% include '::shopHeader.html.twig' %}
    <section>
        {% block slider %}{% endblock %}
        <div class="container_8 clearfix">
            <section class="main-section grid_8">
                <div class="main-content">
                    <div id="search-container" class="container_8 clearfix ui-helper-hidden">
                        <header class="no-margin-bottom">
                            <button class="button ui-corner-none ui-corner-bottom ui-button action-cancel-search ui-button-red fr" data-icon-primary="ui-icon-closethick">Close</button>
                            <h2 class="padding-top-10" id="search-results-title">Search Results</h2>
                        </header>
                        <section class="container_6 clearfix">
                            <div class="grid_6">
                                <div class="portlet">
                                    <section class="padding-bottom-5 no-padding-right">
                                        <div id="search-results"></div>
                                    </section>
                                </div>
                            </div>
                            <div class="clear"></div>
                        </section>
                    </div>
                    {% block header %}{% endblock %}
                    {% block body %}{% endblock %}
                </div>
            </section>
        </div>
    </section>
</div>
<div id="dialog-image-popup" title="Detailed View" class="ui-helper-hidden">
    <div class="ac">
        <img id="dialog-image-popup-image" src="{{ asset('bundles/webilluminationshop/images/loaders/ajax-bars-loader.gif') }}" border="0" alt="" />
    </div>
</div>
{% include '::shopFooter.html.twig' %}
{% include '::flashMessages.html.twig' %}
{% include '::shopAjaxLoading.html.twig' %}
{% include '::shopLargeImage.html.twig' %}
{% include '::shopShoppingBasketPopup.html.twig' %}
{% include '::shopCookiePolicy.html.twig' %}
{% block javascripts %}{% endblock %}
{% include '::shopScript.js.twig' %}
</body>
</html>