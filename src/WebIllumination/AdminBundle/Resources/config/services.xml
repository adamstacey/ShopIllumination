<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="web_illumination_admin.form.new_product_group"
                 class="WebIllumination\AdminBundle\Form\NewProductGroupType">
            <tag name="form.type" alias="admin_new_product_group" />
        </service>

        <service id="web_illumination_admin.form.flow.new_product_group"
                 class="WebIllumination\AdminBundle\Form\NewProductGroupFlow"
                 parent="craue.form.flow"
                 scope="request">
            <call method="setFormType">
                <argument type="service" id="web_illumination_admin.form.new_product_group" />
            </call>
        </service>
    </services>
</container>
