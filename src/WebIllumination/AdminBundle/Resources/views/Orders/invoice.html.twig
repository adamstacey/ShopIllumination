<table cellspacing="0" border="0" style="background-color: #FFFFFF;" cellpadding="0" width="100%">
	<tr>
		<td valign="top">
			<table width="100%" cellspacing="0" border="0" align="center" style="background: #FFFFFF; border: 1px solid #CCCCCC;" cellpadding="0">
				<tr>
					<td valign="top">
						<table width="100%" cellspacing="0" border="0" cellpadding="0">
							<tr>
								<td width="100%" valign="top" style="padding: 20px;">
									<table width="100%" style="border-collapse: collapse;" cellspacing="0" border="0" cellpadding="0">
										<tr>
											<td width="300" valign="top" align="left" style="background: #FFFFFF;"><img height="50" alt="Kitchen Appliance Centre" border="0" src="http://www.kitchenappliancecentre.co.uk/bundles/webilluminationshop/images/logos/kitchen-appliance-centre-email-logo.png" /></td>
											<td valign="top" align="right" style="line-height: 120%; background: #FFFFFF; font-weight: bold; color: #000000; font-size: 24px; font-family: Arial;">INVOICE: <span style="line-height: 120%; font-weight: normal; color: #444444; font-size: 24px; font-family: Arial;">{{ order.orderNumber }}</span><br /><span style="line-height: 120%; font-weight: normal; color: #676767; font-size: 16px; font-family: Arial;">{{ order.orderDate|date('d/m/Y h:iA') }}</span></</td>
										</tr>
									</table>
									<table width="100%" style="border-collapse: collapse;" cellspacing="0" border="0" cellpadding="0">
										<tr>
											<td width="330" valign="top" align="left" style="background: #FFFFFF; padding: 0 20px 0 0;">
												<table width="100%" style="border-collapse: collapse;" cellspacing="0" border="0" cellpadding="0">
													<tr>
														<td valign="top" align="left" style="background: #FFFFFF;">
															<h3 style="border-bottom: 1px dotted #676767; line-height: 100%; color: #000; font-size: 14px; font-family: Arial; padding: 20px 0px 5px 0px; margin: 0px 0px 10px 0px;">Billing Address</h3>
														</td>
													</tr>
													<tr>
														<td valign="top" align="left" style="padding: 50px 0 0 30px; line-height: 120%; background: #FFFFFF; font-weight: normal; color: #000000; text-transform: uppercase; font-size: 12px; font-family: Arial;">{% spaceless %}
															{{ order.billingFirstName }} {{ order.billingLastName }}<br />
															{% if order.billingOrganisationName != '' %}
																{{ order.billingOrganisationName }}<br />
															{% endif %}
															{{ order.billingAddressLine1 }}<br />
															{% if order.billingAddressLine2 != '' %}
																{{ order.billingAddressLine2 }}<br />
															{% endif %}
															{{ order.billingTownCity }}<br />
															{% if order.billingCountyState != '' %}
																{{ order.billingCountyState }}<br />
															{% endif %}
															{% if order.billingCountryCode == 'GB' %}
																{{ order.billingPostZipCode }}<br />
															{% endif %}
															{% if order.billingCountryCode == 'IE' %}
																Republic of Ireland
															{% endif %}
														{% endspaceless %}</td>
													</tr>
												</table>
											</td>
											<td valign="top">
												<table width="100%" style="border-collapse: collapse;" cellspacing="0" border="0" cellpadding="0">
													<tr>
														<td valign="top" align="left" style="background: #FFFFFF;">
															<h3 style="border-bottom: 1px dotted #676767; line-height: 100%; color: #000; font-size: 14px; font-family: Arial; padding: 20px 0px 5px 0px; margin: 0px 0px 10px 0px;">Details</h3>
