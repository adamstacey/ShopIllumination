									{% endif %}
									{% if order.deliveryType == 'Collection' %}
										<p style="font-family: Arial; font-weight: bold; font-size: 11px; color: #CC0000; padding: 20px 0 0 0; margin: 0; line-height: 120%;">PLEASE NOTE: YOU HAVE CHOSEN TO COLLECT YOUR ORDER. WE WILL CONTACT YOU AS SOON AS YOUR ORDER IS READY FOR COLLECTION.</p>
									{% endif %}
									{% if order.customerNotes|length > 0 %}
										<h3 style="border-bottom: 1px dotted #676767; line-height: 100%; color: #000; font-size: 14px; font-family: Arial; padding: 20px 0px 5px 0px; margin: 0px 0px 0px 0px;">Notes</h3>
										{% for note in order.customerNotes %}
											<p style="font-family: Arial; font-size: 11px; color: #CC0000; padding: 10px 0 0 0; margin: 0; line-height: 100%;"><em>{{ note.note }}</em></p>
										{% endfor %}
									{% endif %}
									<h3 style="border-bottom: 1px dotted #676767; line-height: 100%; color: #000; font-size: 14px; font-family: Arial; padding: 20px 0px 5px 0px; margin: 0px 0px 10px 0px;">Order</h3>
									<table style="border-collapse: collapse; border: 1px solid #CCC;" cellspacing="0" border="0" cellpadding="0" width="100%">
										<tr>
											<td width="1" colspan="2" valign="top" align="left" style="border: 1px solid #CCC; text-align: center; line-height: 120%; background: #DDDDDD; color: #000; font-size: 11px; font-family: Arial; padding: 5px 10px; font-weight: bold;" align="center" width="180"><strong>Qty</strong></td>
											<td valign="top" align="left" style="border: 1px solid #CCC; text-align: center; line-height: 120%; background: #DDDDDD; color: #000; font-size: 11px; font-family: Arial; padding: 5px 10px; font-weight: bold;" align="center" width="180"><strong>Product</strong></td>
											<td width="1" valign="top" align="left" style="border: 1px solid #CCC; text-align: center; line-height: 120%; background: #DDDDDD; color: #000; font-size: 11px; font-family: Arial; padding: 5px 10px; font-weight: bold;" align="center" width="180"><strong>Code</strong></td>
											<td width="1" colspan="3" valign="top" align="left" style="border: 1px solid #CCC; text-align: center; line-height: 120%; background: #DDDDDD; color: #000; font-size: 11px; font-family: Arial; padding: 5px 10px; font-weight: bold;" align="center" width="180"><strong>Price</strong></td>
										</tr>
										{% for product in order.products %}
											<tr>
												<td valign="top" style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 5px 10px; text-align: center;" align="center" width="1">{{ product.quantity }}</td>
												<td valign="top" style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 5px 10px; text-align: center;" align="center" width="1">&times;</td>
												<td valign="top" align="left" style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 5px 10px;">{% spaceless %}
													<strong>{{ product.header }}</strong>
													{% if product.selectedOptionLabels|length > 0 %}
														<br />
														{% for selectedOptionLabel in product.selectedOptionLabels %}
															{% if selectedOptionLabel != '' %}
																&nbsp;&nbsp;-&nbsp;&nbsp;{{ selectedOptionLabel|raw }}{% if not loop.last %}<br />{% endif %}
															{% endif %}
														{% endfor %}
													{% endif %}
												{% endspaceless %}</td>
												<td valign="top" style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 5px 10px; text-align: center;" align="center" width="1">{{ product.productCode }}</td>
												<td valign="top" style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 5px 10px; text-align: center;" align="center" width="1">&pound;{{ product.unitCost|number_format(2) }}</td>
												<td valign="top" style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 5px 10px; text-align: center;" align="center" width="1">=</td>
												<td valign="top" style="border: 1px solid #CCC; line-height: 120%; background: #FFFFFF; color: #000; font-size: 11px; font-family: Arial; padding: 5px 10px; text-align: center;" align="center" width="1">&pound;{{ product.subTotal|number_format(2) }}</td>
											</tr>
										{% endfor %}
