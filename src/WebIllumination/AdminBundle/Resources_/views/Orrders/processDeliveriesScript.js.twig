<script type="text/javascript" defer="defer">
	$(document).ready(function() {
		$(".delivery-service").live('change', function() {
			if ($(this).val() == 'Economy')
			{
				$("#listing-delivery-dates-"+$(this).attr("data-id")).hide();
			} else {
				$("#listing-delivery-dates-"+$(this).attr("data-id")).show();
			}
		});
	});
</script>