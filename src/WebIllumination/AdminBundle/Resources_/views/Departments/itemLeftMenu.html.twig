{% if data.mode == 'update' %}
	<li{% if (app.request.attributes.get('_route') == 'admin_'~data.settings.multipleClass~'_update') %} class="current"{% endif %}><a class="navicon-set-white navicon-notepad" href="{{ path('admin_'~data.settings.multipleClass~'_update', {'id': data.item.departmentId}) }}">General</a></li>
	{% if data.item.directProductCount > 0 %}
		<li{% if (app.request.attributes.get('_route') == 'admin_'~data.settings.multipleClass~'_update_templates') %} class="current"{% endif %}><a class="navicon-set-white navicon-template" href="{{ path('admin_'~data.settings.multipleClass~'_update_templates', {'id': data.item.departmentId}) }}">Templates</a></li>
	{% endif %}
