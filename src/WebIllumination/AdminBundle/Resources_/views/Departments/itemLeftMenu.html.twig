	{% if data.item.directProductCount > 0 %}
		<li{% if (app.request.attributes.get('_route') == 'admin_'~data.settings.multipleClass~'_update_product_features') %} class="current"{% endif %}><a class="navicon-set-white navicon-list" href="{{ path('admin_'~data.settings.multipleClass~'_update_product_features', {'id': data.item.departmentId}) }}">Product Features</a></li>
